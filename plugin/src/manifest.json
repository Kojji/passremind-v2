{
  "name": "PassRemind",
  "description": "Browser extension for the PassRemind web app.",
  "version": "2.0.0",
  "manifest_version": 3,
  "icons": {
    "48": "assets/pssrLogo-48.png",
    "128": "assets/pssrLogo-128.png"
  },
  "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["content-scripts/main.js"]
    }
  ]
}